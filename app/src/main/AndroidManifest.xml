<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="retrofit.app.com.androidretrofitexample" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.app.retrofit.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.app.retrofit.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data android:name="AA_DB_NAME" android:value="eventeye_db.db" />
       <!-- <meta-data android:name="AA_MODELS" android:value="com.app.retrofit.Models.Tags,com.app.retrofit.Models.Participants,com.app.retrofit.Models.Category,com.app.retrofit.Models.Item, com.app.retrofit.Models.ParticipantsTags" /> -->
        <meta-data android:name="AA_DB_VERSION" android:value="1" />
    </application>

</manifest>
